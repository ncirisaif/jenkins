pipeline{

    agent any
        tools {
            maven 'Maven 3.9.1'
            jdk 'Java 17.0.4.1'
        }

    stages{

        stage('git Checkout'){
            steps{
                git branch: 'master', url: 'https://github.com/ncirisaif/jenkins.git'
            }
        }
                stage('Unit test'){
                    steps{
                        sh 'mvn test'
                    }
                }

    }

}
pipeline{

    agent any
        tools {
            maven 'maven'
            jdk 'Java17'

        }

    stages{

        stage('git Checkout'){
            steps{
                git branch: 'master', url: 'https://github.com/ncirisaif/jenkins.git'
            }
        }
                stage('Unit test'){
                    steps{
                        sh 'mvn test'
                    }
                }

    }

}